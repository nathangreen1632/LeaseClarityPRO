{
  "name": "digital-lease-reader",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "workspaces": [
    "Client",
    "Server"
  ],
  "scripts": {
    "build": "npm run build --workspace=Server && npm run build --workspace=Client",
    "dev": "concurrently \"npm run dev --workspace=Client\" \"npm run dev --workspace=Server\"",
    "start": "npm run build && concurrently \"npm run start --workspace=Server\" \"npm run preview --workspace=Client\"",
    "postinstall": "npm install --workspace=Server --ignore-scripts && npm install --workspace=Client --ignore-scripts",
    "preview": "npm run preview --workspace=Client"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1"
  }
}
